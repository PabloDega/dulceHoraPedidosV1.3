<form action="" method="post" id="stock" class="editor">
  <h1>Stock de <%= data.nombre %></h1>
  <table>
    <tbody>
    <%  let dataStock = data.stock.split(",");
        categorias.forEach((dato) => {
          let buscar = productos.filter((x) => x.categoria == dato.categoria)%>
            <tr>
              <td class="tablaCategoria"><%= dato.categoria %></td>
            </tr>
          <%buscar.forEach((dato) => {
            let checked;
            if(dataStock.find((x) => x == dato.id)){
              checked = "checked";
            }else{
              checked = "unchecked";
            }%>
            <tr class="fila <%= checked %>" id="fila<%= dato.id %>">
              <td>
                <label for="check<%= dato.id %>"><%= dato.nombre %></label>
                <input type="checkbox" name="stock" value="<%= dato.id %>" id="check<%= dato.id %>" <%= checked %>>
              </td>
            </tr>  
          <%})
      }) %>
    </tbody>
  </table>
  <input type="submit" value="Actualizar">
</form>