<form action="" method="post" id="nuevoLocal" class="editor">
    <h1>Nuevo Local</h1>
    <span>
        <label for="id">ID</label>
        <input type="text" name="id" id="id" value="<%= data %>" readonly />
    </span>
    <span>
        <label for="franquicia">Franquicia</label>
        <input type="text" name="franquicia" id="franquicia" value="<%= valoresForm.franquicia %>" />
    </span>
    <span>
        <label for="nombre">Nombre</label>
        <input type="text" name="nombre" id="nombre" value="<%= valoresForm.nombre %>" />
    </span>
    <span>
        <label for="direccion">Dirección</label>
        <input type="text" name="direccion" id="direccion" value="<%= valoresForm.direccion %>" />
    </span>
    <span>
        <label for="telefono">Teléfono</label>
        <input type="text" name="telefono" id="telefono" value="<%= valoresForm.telefono %>" />
    </span>
    <span>
        <label for="gmap">Ubicación Google Maps</label>
        <textarea name="gmap" id="gmap" rows="8"><%= valoresForm.gmap %></textarea>
    </span>
    <span>
        <label for="img">Nombre de Imagen</label>
        <input type="text" name="img" id="img" value="im/local/local<%= data %>.png" readonly />
    </span>
    <span>
        <label for="linkmp">Link MP</label>
        <input type="text" name="linkmp" id="linkmp" value="<%= valoresForm.linkmp %>" />
    </span>
    <span>
        <span>Servicios</span>
        <span>
            <div id="nuevoLocalServicios">
            <% servicios.forEach((servicio) => {%>
                <div>
                    <span><input type="checkbox" name="<%= servicio.servicio %>" id="" value="true"></span>
                    <span>Servicio <%= servicio.servicio %></span>
                </div>
            <%})%>
            </div>
        </span>
    </span>
    
    <span>
        <input type="submit" value="Crear">
    </span>
    <input type="hidden" name="tipo" value="nuevo">
</form>

